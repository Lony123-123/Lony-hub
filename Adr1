local urls = {
    "https://raw.githubusercontent.com/Lony123-123/Lony-hub/refs/heads/main/Slap1",       -- Script A
    "https://raw.githubusercontent.com/Lony123-123/Lony-hub/refs/heads/main/Esp-name",    -- Script B
    "https://raw.githubusercontent.com/Lony123-123/Lony-hub/refs/heads/main/Get-slap",    -- Script C
    "https://raw.githubusercontent.com/Lony123-123/Lony-hub/refs/heads/main/Delteleport", -- Script D
    "https://raw.githubusercontent.com/Lony123-123/Lony-hub/refs/heads/main/Delebuyshop", -- Script E
}

for _, url in ipairs(urls) do
    task.spawn(function()
        local s, err = pcall(function()
            loadstring(game:HttpGet(url))()
        end)
        if not s then
            warn("L·ªói khi ch·∫°y script t·ª´: " .. url .. " | " .. err)
        end
    end)
end
